From 79835e6f982d7ab3a02e8df4a39851bb77f0a196 Mon Sep 17 00:00:00 2001
From: ksrt12 <kazakov12stepan2012@live.ru>
Date: Fri, 23 Sep 2016 11:49:42 +0300
Subject: [PATCH] fix cortex-a5

Change-Id: Ic3b2c5c2f45d577aded1e7a324d0858d828daee1
---
 core/combo/arch/arm/armv7-a-neon.mk | 2 +-
 target/product/full_base.mk         | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/core/combo/arch/arm/armv7-a-neon.mk b/core/combo/arch/arm/armv7-a-neon.mk
index a49b7f0..8879043 100644
--- a/core/combo/arch/arm/armv7-a-neon.mk
+++ b/core/combo/arch/arm/armv7-a-neon.mk
@@ -31,7 +31,7 @@ ifeq ($(strip $(TARGET_$(combo_2nd_arch_prefix)CPU_VARIANT)),cortex-a7)
 		-Wl,--no-fix-cortex-a8
 else
 ifeq ($(strip $(TARGET_$(combo_2nd_arch_prefix)CPU_VARIANT)),cortex-a5)
-	arch_variant_cflags := -mcpu=cortex-a7
+	arch_variant_cflags := -mcpu=cortex-a5
 	arch_variant_ldflags := \
 		-Wl,--no-fix-cortex-a8
 else
diff --git a/target/product/full_base.mk b/target/product/full_base.mk
index f9b8505..294286a 100644
--- a/target/product/full_base.mk
+++ b/target/product/full_base.mk
@@ -28,7 +28,7 @@ PRODUCT_PROPERTY_OVERRIDES := \
     ro.com.android.dateformat=MM-dd-yyyy
 
 # Put en_US first in the list, so make it default.
-PRODUCT_LOCALES := en_US
+PRODUCT_LOCALES := ru_RU
 
 # Get some sounds
 $(call inherit-product-if-exists, frameworks/base/data/sounds/AllAudio.mk)
-- 
