<qosrules>
  
  <!-- Ondemand -->
  <rule>
    <booleanFilter>
      <and>
	<booleanFilter>
	  <equality>
	    <attribute>
	      <attributeId>/sys/power/wait_for_fb_status</attributeId>
	      <attributeValue>
		<string>on</string>
	      </attributeValue>
	    </attribute>
	  </equality>
	</booleanFilter>
	<booleanFilter>
	  <equality>
	    <attribute>
	      <attributeId>/sys/devices/system/cpu/cpu0/cpufreq/scaling_governor</attributeId>
	      <attributeValue>
		<string>ondemand</string>
	      </attributeValue>
	    </attribute>
	  </equality>
	</booleanFilter>
      </and>
    </booleanFilter>
    <assignment>
      <attribute>
	<attributeId>/sys/devices/system/cpu/cpufreq/ondemand/sampling_rate</attributeId>
	<attributeValue>
	  <string>25000</string>
	</attributeValue>
      </attribute>
    </assignment>
  </rule>
  <rule>
    <booleanFilter>
      <and>
	<booleanFilter>
	  <equality>
	    <attribute>
	      <attributeId>/sys/power/wait_for_fb_status</attributeId>
	      <attributeValue>
		<string>off</string>
	      </attributeValue>
	    </attribute>
	  </equality>
	</booleanFilter>
	<booleanFilter>
	  <equality>
	    <attribute>
	      <attributeId>/sys/devices/system/cpu/cpu0/cpufreq/scaling_governor</attributeId>
	      <attributeValue>
		<string>ondemand</string>
	      </attributeValue>
	    </attribute>
	  </equality>
	</booleanFilter>
      </and>
    </booleanFilter>
    <assignment>
      <attribute>
	<attributeId>/sys/devices/system/cpu/cpufreq/ondemand/sampling_rate</attributeId>
	<attributeValue>
	  <string>700000</string>
	</attributeValue>
      </attribute>
    </assignment>
  </rule>

  <!-- Intellidemand -->
  <rule>
    <booleanFilter>
      <and>
	<booleanFilter>
	  <equality>
	    <attribute>
	      <attributeId>/sys/power/wait_for_fb_status</attributeId>
	      <attributeValue>
		<string>on</string>
	      </attributeValue>
	    </attribute>
	  </equality>
	</booleanFilter>
	<booleanFilter>
	  <equality>
	    <attribute>
	      <attributeId>/sys/devices/system/cpu/cpu0/cpufreq/scaling_governor</attributeId>
	      <attributeValue>
		<string>intellidemand</string>
	      </attributeValue>
	    </attribute>
	  </equality>
	</booleanFilter>
      </and>
    </booleanFilter>
    <assignment>
      <attribute>
	<attributeId>/sys/devices/system/cpu/cpufreq/intellidemand/sampling_rate</attributeId>
	<attributeValue>
	  <string>25000</string>
	</attributeValue>
      </attribute>
    </assignment>
  </rule>
  <rule>
    <booleanFilter>
      <and>
	<booleanFilter>
	  <equality>
	    <attribute>
	      <attributeId>/sys/power/wait_for_fb_status</attributeId>
	      <attributeValue>
		<string>off</string>
	      </attributeValue>
	    </attribute>
	  </equality>
	</booleanFilter>
	<booleanFilter>
	  <equality>
	    <attribute>
	      <attributeId>/sys/devices/system/cpu/cpu0/cpufreq/scaling_governor</attributeId>
	      <attributeValue>
		<string>intellidemand</string>
	      </attributeValue>
	    </attribute>
	  </equality>
	</booleanFilter>
      </and>
    </booleanFilter>
    <assignment>
      <attribute>
	<attributeId>/sys/devices/system/cpu/cpufreq/intellidemand/sampling_rate</attributeId>
	<attributeValue>
	  <string>700000</string>
	</attributeValue>
      </attribute>
    </assignment>
  </rule>
  
 </qosrules>
